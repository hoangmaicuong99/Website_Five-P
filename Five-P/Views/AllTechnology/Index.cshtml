@using Five_P.Models
@model List<Technology>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/AllTechnology/Index.css" rel="stylesheet" />
<style>
</style>
<div class="social-box " style="margin-top:100px">
    <div class="container">
        <div class="row">
            <h2>Tất cả công nghệ</h2>
        </div>
        <div class="row">
            <div class="col-lg-9">
                <div class="row">
                    @foreach (var item in Model)
                    {
                        <div class="col-lg-4 col-xs-12 text-center">
                            <div class="box">
                                <div class="box-title">
                                    <h3>@item.technology_name</h3>
                                </div>
                                <div class="box-text">
                                    <span>Elitr scripta ocurreret qui ad.</span>
                                </div>
                                <div class="box-btn">
                                    <a href="#">Câu hỏi</a>
                                </div>
                            </div>
                        </div>
                    }

                </div>
            </div>
            <div class="col-lg-3">

                <img src="~/Content/ImgAdvertisement/quangCao2.jpg" class="w-100"/>
            </div>
        </div>
    </div>
</div>

